{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["tsconfig.json"],
  "ui": "tui",
  "tasks": {
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "generate": {
      "persistent": true,
      "cache": false
    },
    "migrate-dev": {
      "interactive": true,
      "cache": false
    },
    "migrate-deploy": {
      "interactive": true,
      "cache": false
    },
    "push": {
      "persistent": false,
      "cache": false
    },
    "pull": {
      "persistent": false,
      "cache": false
    },
    "seed": {
      "persistent": false,
      "cache": false
    },
    "validate": {
      "interactive": true,
      "cache": false
    }
  }
}
