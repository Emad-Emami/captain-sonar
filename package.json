{
  "name": "captain-sonar",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev:server": "turbo run dev --filter=@cs/server",
    "dev:web": "turbo run dev --filter=@cs/web",
    "dev": "turbo run dev --filter=@cs/web",
    "db:validate": "turbo run validate --filter=@cs/db",
    "db:generate": "turbo run generate --filter=@cs/db",
    "db:push": "turbo run push --filter=@cs/db",
    "db:pull": "turbo run pull --filter=@cs/db",
    "db:seed": "turbo run seed --filter=@cs/db",
    "db:migrate-dev": "turbo run migrate-dev --filter=@cs/db",
    "db:migrate-deploy": "turbo run migrate-deploy --filter=@cs/db",
    "db:migrate-create-only": "turbo run migrate --only --filter=@cs/db -- --create-only",
    "db:migrate-reset": "turbo run migrate --only --filter=@cs/db -- --reset",
    "db:migrate-force-reset": "turbo run migrate --only --filter=@cs/db -- --force-reset",
    "lint": "eslint --ext \".js,.ts,.tsx\" --report-unused-disable-directives */*.ts"
  },
  "workspaces": [
    "packages/web",
    "packages/server",
    "packages/db"
  ],
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@types/react": "18.3.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-mantine": "^3.2.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.0",
    "turbo": "^2.2.3",
    "typescript": "5.5.4",
    "wait-port": "^1.1.0"
  },
  "imports": {
    "@CS/web": "./packages/web/*",
    "@CS/server": "./packages/server/src/*",
    "@CS/db": "./packages/db/*"
  },
  "dependencies": {},
  "packageManager": "npm@10.2.4",
  "author": "Emad Emami",
  "license": "ISC"
}
