{
  "name": "captain-sonar",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "web:dev": "turbo run dev --filter=@cs/web",
    "web:build": "turbo run build --filter=@cs/web",
    "web:start": "turbo run start --filter=@cs/web",
    "server:dev": "turbo run dev --filter=@cs/server",
    "db:validate": "turbo run validate --filter=@cs/db",
    "db:generate": "turbo run generate --filter=@cs/db",
    "db:push": "turbo run push --filter=@cs/db",
    "db:pull": "turbo run pull --filter=@cs/db",
    "db:seed": "turbo run seed --filter=@cs/db",
    "db:migrate-dev": "turbo run migrate-dev --filter=@cs/db",
    "db:migrate-deploy": "turbo run migrate-deploy --filter=@cs/db",
    "db:migrate-create-only": "turbo run migrate --only --filter=@cs/db -- --create-only",
    "db:migrate-reset": "turbo run migrate --only --filter=@cs/db -- --reset",
    "db:migrate-force-reset": "turbo run migrate --only --filter=@cs/db -- --force-reset",
    "lint": "eslint --ext \".js,.ts,.tsx\" --report-unused-disable-directives */*.ts"
  },
  "workspaces": [
    "@CS/web",
    "packages/server",
    "packages/db"
  ],
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.25.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.25.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.0",
    "turbo": "^2.2.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wait-port": "^1.1.0"
  },
  "imports": {
    "@CS/web": "./@CS/web/*",
    "@CS/server": "./packages/server/src/*",
    "@CS/db": "./packages/db/*"
  },
  "dependencies": {},
  "packageManager": "npm@10.2.4",
